from . import sequencer
from . import core
import multiprocessing as mp


class Machine(object):

    def __init__(self):
        data = core.Data()
        self.processes = list()
        
        self.processes.append(
            mp.Process(name='seq',
                       target=sequencer.Sequencer,
                       args=(data,)))

        self.processes.append(
            mp.Process(name='seq2',
                       target=sequencer.Sequencer,
                       args=(data,)))

        for iproc in self.processes:
            iproc.start()

        for iproc in self.processes:
            iproc.join()
        
        

